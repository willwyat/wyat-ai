# Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_FRONTEND_ORIGIN=http://localhost:3000

# Wyat API Key (must match backend WYAT_API_KEY)
NEXT_PUBLIC_WYAT_API_KEY=your-secure-api-key-here

# Blockchain Explorer URLs (Optional)
NEXT_PUBLIC_EVM_EXPLORER=https://etherscan.io/address/
NEXT_PUBLIC_SOLANA_EXPLORER=https://solscan.io/account/
NEXT_PUBLIC_BITCOIN_EXPLORER=https://blockchair.com/bitcoin/address/

